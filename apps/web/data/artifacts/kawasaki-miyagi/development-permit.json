            "ordered": false,
            "items": [
              [
                {
                  "type": "paragraph",
                  "runs": [
                    {
                      "text": "市街化区域",
                      "bold": true
                    },
                    {
                      "text": "：1,000㎡以上（三大都市圏は500㎡以上）"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "runs": [
                    {
                      "text": "市街化調整区域",
                      "bold": true
                    },
                    {
                      "text": "：原則としてすべての開発行為"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "runs": [
                    {
                      "text": "非線引き区域",
                      "bold": true
                    },
                    {
                      "text": "：3,000㎡以上"
                    }
                  ]
                }
              ],
              [
                {
                  "type": "paragraph",
                  "runs": [
                    {
                      "text": "都市計画区域外",
                      "bold": true
                    },
                    {
                      "text": "：10,000㎡以上"
                    }
                  ]
                }
              ]
            ]
          }
        ]
      }
    },
